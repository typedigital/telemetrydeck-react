{
    "name": "@typedigital/telemetrydeck-react",
    "version": "0.2.0",
    "author": "Andreas Kottre",
    "module": "dist/telemetrydeck-react.esm.js",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "prepublish": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "eslint ./src",
        "lint-fix": "eslint ./src --fix",
        "prepare": "npm run build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": ">= 16.8.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "eslint ./src"
        }
    },
    "size-limit": [
        {
            "path": "dist/telemetrydeck-react.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/telemetrydeck-react.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^9.0.0",
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/react": "^16.0.0",
        "@testing-library/user-event": "^14.5.2",
        "@typedigital/eslint-config": "^7.3.2",
        "@types/react": "^18.0.35",
        "@types/react-dom": "^18.0.11",
        "@types/text-encoding": "^0.0.39",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "cross-fetch": "^4.0.0",
        "eslint": "^8.38.0",
        "headers-polyfill": "^3.2.5",
        "husky": "^8.0.3",
        "msw": "^0.42.3",
        "react": "^18.2.0",
        "react-dom": "^18.3.1",
        "size-limit": "^9.0.0",
        "text-encoding": "^0.7.0",
        "tsdx": "^0.14.1",
        "tslib": "^2.5.0",
        "typescript": "4.2"
    },
    "dependencies": {
        "@telemetrydeck/sdk": "^2.0.4"
    }
}
